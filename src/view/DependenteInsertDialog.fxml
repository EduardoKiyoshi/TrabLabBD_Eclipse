<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.DependenteInsertDialogController">
   <children>
      <TextField fx:id="nomeCompletoDe" layoutX="192.0" layoutY="86.0" />
      <TextField fx:id="birthdayField" layoutX="190.0" layoutY="128.0" />
      <Label layoutX="35.0" layoutY="90.0" text="Nome do dependente" />
      <Label layoutX="46.0" layoutY="132.0" text="Data do Aniversário" />
      <Label layoutX="120.0" layoutY="175.0" text="Sexo" />
      <ButtonBar layoutX="386.0" layoutY="249.0" prefHeight="40.0" prefWidth="200.0">
        <buttons>
          <Button mnemonicParsing="false" onAction="#handleOk" text="Cadastrar" />
            <Button mnemonicParsing="false" onAction="#handleCancel" text="Cancelar" />
        </buttons>
      </ButtonBar>
      <RadioButton fx:id="rMas" layoutX="188.0" layoutY="175.0" mnemonicParsing="false" text="Masculino">
         <toggleGroup>
            <ToggleGroup fx:id="sexoDe" />
         </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="rFem" layoutX="300.0" layoutY="175.0" mnemonicParsing="false" text="Feminino" toggleGroup="$sexoDe" />
      <Label layoutX="35.0" layoutY="205.0" text="Campos marcados com * são obrigatórios" />
      <Label layoutX="349.0" layoutY="90.0" text="*" />
   </children>
</AnchorPane>
